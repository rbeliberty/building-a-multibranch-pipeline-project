
pipeline {
    agent none

    stages {

        stage('Build Image and Push Registry') {
            agent any
            steps {
                withEnv(['PATH+EXTRA=/usr/sbin:/usr/bin:/sbin:/bin']) {

                    sh '''
                        ls -la
                    '''
                }
            }
        }

    }
    post {
        always {
            cleanWs()
        }
    }
}


